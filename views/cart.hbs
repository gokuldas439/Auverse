        {{else}}
        <div class="row">
          <span style="color: red;"><b>{{this.stockErr}}</b></span>
          <div class="col-12 col-md-7">

            <!-- List group -->
            <ul class="list-group list-group-lg list-group-flush-x mb-6">
              {{#each cartProducts}}
              <li class="list-group-item">
                <div class="row align-items-center">
                  <div class="col-3">

                    <!-- Image -->
                    <a href="/products/view-product/{{this.productDetails._id}}">
                      <img src="{{this.productDetails.images.[1]}}" alt="..." class="img-fluid">
                    </a>

                  </div>
                  <div class="col">

                    <!-- Title -->
                    <div class="d-flex mb-2 fw-bold">
                      <a class="text-body" href="/products/view-product/{{this.productDetails._id}}">{{this.productDetails.name}}</a>
                     <span class="totalprice ms-auto {{this.productDetails._id}}" style="color: rgb(0 159 0);">Rs. {{toLocaleString this.subTotal}}</span>
                    </div>

                    <!-- Text -->
                    <p class="mb-7 fs-sm text-muted">
                      
                     <span><b> Color: {{this.productDetails.color}}</b><br></span>
                      {{#eq this.productDetails.stocks 0}}
                      <span style="color: red;"><b> stocks: ( Out of Stocks )</b><br></span>
                      {{else}}
                     <span style="color: green;"><b> stocks: {{this.productDetails.stocks}}</b><br></span>
                      {{/eq}}
                    </p>
                    <span class="ms-auto" style="color: rgb(0, 0, 0);"> Price per Unit : Rs. {{toLocaleString this.productDetails.offerprice}}</span>

                    <!--Footer -->
                    <div class="d-flex align-items-center">

                      <!-- Select -->
                      <div class="number">
	                    <span class="minus" onclick="changeQuantity('{{this.productDetails.offerprice}}','{{this.productDetails._id}}',-1)">-</span>
	                    <input type="text" class="cartquantity" id="{{this.productDetails._id}}" value="{{this.count}}" disabled/>
                      <span class="plus" onclick="changeQuantity('{{this.productDetails.offerprice}}','{{this.productDetails._id}}',1)">+</span>
                      </div>

                      

                      <!-- Remove -->
                      <a class="fs-xs text-gray-400 ms-auto" href="/removeCart/{{this.productDetails._id}}">
                        <i class="fe fe-x"></i> Remove
                      </a>

                    </div>

                  </div>
                </div>
              </li>
              {{/each}}
            </ul>

            <!-- Footer -->
            

          </div>
          <div class="col-12 col-md-5 col-lg-4 offset-lg-1">

            <!-- Total -->
            <div class="card mb-7 bg-light" style="padding: 1rem;">
              <div class="card-body">
                <ul class="list-group list-group-sm list-group-flush-y list-group-flush-x">
                  <li class="list-group-item d-flex">
                    {{#with carttotal}}
                    <span>Subtotal</span> <span class="ms-auto fs-sm" id="overall">Rs. {{toLocaleString this.total}}</span>
                  </li>
                  <li class="list-group-item d-flex">
                    <span>Other Discounts</span> <span class="ms-auto fs-sm" style="color: green;">Rs. 0</span>
                  </li>
                  <li class="list-group-item d-flex fs-lg fw-bold">
                    <span>Total</span>
                     <span class="ms-auto fs-sm" id="totalspan">Rs. {{toLocaleString this.total}}</span>
                     {{/with}}
                  </li>
                  <li class="list-group-item fs-sm text-center text-gray-500">
                    Shipping cost calculated at Checkout *
                  </li>
                </ul>
              </div>
            </div>

            <!-- Button -->
            <a class="btn w-100 btn-dark mb-2" href="/checkout">Proceed to Checkout</a>

            <!-- Link -->
            <a class="btn btn-link btn-sm px-0 text-body" href="/products">
              <i class="fe fe-arrow-left me-2"></i> Continue Shopping
            </a>

          </div>
